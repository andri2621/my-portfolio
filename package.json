    "clsx": "^1.2.1",
    "concurrently": "^8.2.2",
    "contentlayer": "^0.3.4",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.260.0",
    "next": "^13.5.4",
    "next-cloudinary": "^4.28.0",
